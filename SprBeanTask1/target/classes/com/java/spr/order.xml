<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
"http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="productOne" class="com.java.spr.Product">
		<property name="product" value="Phone"/>
		<property name="quantityavail" value="30"/>
		<property name="price" value="35000"/>
	</bean>
	<bean id="orderOne" class="com.java.spr.Order">
		<property name="qtyord" value="1"/>
		<property name="orderid" value="1"/>
		<property name="objproduct" ref="productOne"/>
	</bean>
	
	<bean id="productTwo" class="com.java.spr.Product">
		<property name="product" value="Phone"/>
		<property name="quantityavail" value="30"/>
		<property name="price" value="75000"/>
	</bean>
	<bean id="orderTwo" class="com.java.spr.Order">
		<property name="qtyord" value="4"/>
		<property name="orderid" value="2"/>
		<property name="objproduct" ref="productTwo"/>
	</bean>
	
	<bean id="productThree" class="com.java.spr.Product">
		<property name="product" value="Phone"/>
		<property name="quantityavail" value="40"/>
		<property name="price" value="135000"/>
	</bean>
	<bean id="orderThree" class="com.java.spr.Order">
		<property name="qtyord" value="5"/>
		<property name="orderid" value="3"/>
		<property name="objproduct" ref="productThree"/>
	</bean>
	
	<bean id="orderLogic" class="com.java.spr.OrderLogic">
		<property name="items">
			<list>
				<ref bean="orderOne"></ref>
				<ref bean="orderTwo"></ref>
				<ref bean="orderThree"></ref>
			</list>
		</property>
	</bean>
</beans>