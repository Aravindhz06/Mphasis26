<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
"http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="lunchBox1" class="com.java.spr.LunchBox">
		<property name="snacks" value="biscuit"/>
	</bean>
	<bean id="student1" class="com.java.spr.Student">
		<property name="sno" value="1"/>
		<property name="name" value="Aravindh"/>
		<property name="lunchbox" ref="lunchBox1"/>
	</bean>
	<bean id="Faculty" class="com.java.spr.Faculty">
		<property name="fname" value="Dhoni"/>
		<property name="students">
			<list>
				<ref bean="student1"/>
			</list>
		</property>
	</bean>
</beans>